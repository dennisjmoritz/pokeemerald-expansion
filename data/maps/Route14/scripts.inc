	.string "POKéMON.$"

Route14_Text_JanIntro::
# 145 "data/maps/Route14/scripts.pory"
	.string "JAN: KIRI, here we go! We have to try\n"
	.string "hard!$"

Route14_Text_JanDefeat::
# 149 "data/maps/Route14/scripts.pory"
	.string "JAN: Eeeeh! No fair!$"

Route14_Text_JanPostBattle::
# 153 "data/maps/Route14/scripts.pory"
