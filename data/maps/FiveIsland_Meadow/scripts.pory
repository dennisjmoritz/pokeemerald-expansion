#events for FiveIsland_Meadow

script FiveIsland_Meadow_MapScripts {
	map_script MAP_SCRIPT_ON_LOAD, FiveIsland_Meadow_OnLoad
	.byte 0

}

script FiveIsland_Meadow_OnLoad {
	call_if_set FLAG_TEMP_1, FiveIsland_Meadow_EventScript_SetWarehouseDoorUnlocked
// Replaced FLAG_UNLOCKED_ROCKET_WAREHOUSE with FLAG_TEMP_1
	end

}

script FiveIsland_Meadow_EventScript_WarehouseDoor {
	lockall
	goto_if_set FLAG_TEMP_1, FiveIsland_Meadow_EventScript_WarehouseDoorAlreadyOpen
// Replaced FLAG_UNLOCKED_ROCKET_WAREHOUSE with FLAG_TEMP_1
	goto_if_set FLAG_TEMP_1, FiveIsland_Meadow_EventScript_OpenWarehouseDoor
// Replaced FLAG_LEARNED_YES_NAH_CHANSEY with FLAG_TEMP_1
	msgbox FiveIsland_Meadow_Text_EnteredPasswordAnotherNeeded
	releaseall
	end

}

script FiveIsland_Meadow_EventScript_OpenWarehouseDoor {
	setflag FLAG_TEMP_1
// Replaced FLAG_UNLOCKED_ROCKET_WAREHOUSE with FLAG_TEMP_1
	message FiveIsland_Meadow_Text_EnteredPasswordDoorOpened
	waitmessage
	delay 60
	call FiveIsland_Meadow_EventScript_SetWarehouseDoorUnlocked
	special DrawWholeMapView
	playse SE_PIN
	waitse
	releaseall
	end

}

script FiveIsland_Meadow_EventScript_WarehouseDoorAlreadyOpen {
	msgbox FiveIsland_Meadow_Text_WarehouseDoorAlreadyOpen
	releaseall
	end

}

script FiveIsland_Meadow_EventScript_SetWarehouseDoorUnlocked {
// FIXME undefined reference: 	setmetatile 12, 21, METATILE_SeviiIslands45_RocketWarehouseDoor_Unlocked, 1
	return

}

