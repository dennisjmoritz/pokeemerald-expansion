#events for CeladonCity_Condominiums_1F

script CeladonCity_Condominiums_1F_MapScripts {
	.byte 0

}

script CeladonCity_Condominiums_1F_EventScript_TeaWoman {
	lock
	faceplayer
	goto_if_set FLAG_TEMP_1, CeladonCity_Condominiums_1F_EventScript_TeaWomanAfterTea
// Replaced FLAG_TALKED_TO_TEA_LADY_AFTER_HOF with FLAG_TEMP_1
	goto_if_set FLAG_SYS_GAME_CLEAR, CeladonCity_Condominiums_1F_EventScript_TeaWomanMentionDaisy
	goto_if_set FLAG_TEMP_1, CeladonCity_Condominiums_1F_EventScript_TeaWomanAfterTea
// Replaced FLAG_GOT_TEA with FLAG_TEMP_1
	msgbox CeladonCity_Condominiums_1F_Text_TryThisDrinkInstead
	setflag FLAG_TEMP_1
// Replaced FLAG_GOT_TEA with FLAG_TEMP_1
	giveitem ITEM_TEA
// FIXME undefined reference: 	goto_if_eq VAR_RESULT, FALSE, EventScript_BagIsFull
	msgbox CeladonCity_Condominiums_1F_Text_NothingBeatsThirstLikeTea
	release
	end

}

script CeladonCity_Condominiums_1F_EventScript_TeaWomanAfterTea {
	msgbox CeladonCity_Condominiums_1F_Text_MyDearMonsKeepMeCompany
	release
	end

}

script CeladonCity_Condominiums_1F_EventScript_TeaWomanMentionDaisy {
	setflag FLAG_TEMP_1
// Replaced FLAG_TALKED_TO_TEA_LADY_AFTER_HOF with FLAG_TEMP_1
	msgbox CeladonCity_Condominiums_1F_Text_DaisyComesToBuyTea
	release
	end

}

script CeladonCity_Condominiums_1F_EventScript_Meowth {
	lock
	faceplayer
	waitse
	playmoncry SPECIES_MEOWTH, CRY_MODE_NORMAL
	msgbox CeladonCity_Condominiums_1F_Text_Meowth
	waitmoncry
	release
	end

}

