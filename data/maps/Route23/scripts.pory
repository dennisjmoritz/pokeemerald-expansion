#events for Route23

script Route23_MapScripts {
	map_script MAP_SCRIPT_ON_TRANSITION, Route23_OnTransition
	.byte 0

}

script Route23_OnTransition {
	clearflag FLAG_TEMP_1
// Replaced FLAG_HIDE_VICTORY_ROAD_3F_BOULDER with FLAG_TEMP_1
	setflag FLAG_TEMP_1
// Replaced FLAG_HIDE_VICTORY_ROAD_2F_BOULDER with FLAG_TEMP_1
	setvar VAR_TEMP_0, 0
// Replaced VAR_MAP_SCENE_VICTORY_ROAD_1F with VAR_TEMP_0
	setvar VAR_TEMP_0, 0
// Replaced VAR_MAP_SCENE_VICTORY_ROAD_2F_BOULDER1 with VAR_TEMP_0
	setvar VAR_TEMP_0, 0
// Replaced VAR_MAP_SCENE_VICTORY_ROAD_2F_BOULDER2 with VAR_TEMP_0
	setvar VAR_TEMP_0, 0
// Replaced VAR_MAP_SCENE_VICTORY_ROAD_3F with VAR_TEMP_0
	end

}

