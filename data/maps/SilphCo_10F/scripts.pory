script SilphCo_10F_EventScript_WorkerF {
	lock
	faceplayer
	goto_if_ge VAR_TEMP_0, 1, SilphCo_10F_EventScript_WorkerFRocketsGone
// Replaced VAR_MAP_SCENE_SILPH_CO_11F with VAR_TEMP_0
	msgbox SilphCo_10F_Text_WaaaImScared
	release
	end

}

script SilphCo_10F_EventScript_WorkerFRocketsGone {
	msgbox SilphCo_10F_Text_KeepMeCryingASecret
	release
	end

}

