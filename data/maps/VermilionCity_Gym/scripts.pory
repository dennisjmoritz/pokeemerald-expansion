// Replaced FLAG_HIDE_VERMILION_CITY_OAKS_AIDE with FLAG_TEMP_1
	return

}

script VermilionCity_Gym_EventScript_GiveTM34 {
	msgbox VermilionCity_Gym_Text_ExplainThunderBadgeTakeThis
	checkitemspace ITEM_TM34
	goto_if_eq VAR_RESULT, FALSE, VermilionCity_Gym_EventScript_NoRoomForTM34
	msgbox VermilionCity_Gym_Text_ReceivedTM34FromLtSurge, MSGBOX_DEFAULT
	giveitem ITEM_TM34
	setflag FLAG_TEMP_1
// Replaced FLAG_GOT_TM34_FROM_SURGE with FLAG_TEMP_1
	msgbox VermilionCity_Gym_Text_ExplainTM34
	release
	end

}

script VermilionCity_Gym_EventScript_NoRoomForTM34 {
